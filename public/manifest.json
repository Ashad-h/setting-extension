{
  "manifest_version": 3,
  "name": "LinkedIn Comment Scraper",
  "version": "1.0.0",
  "description": "Scrape and categorize LinkedIn comments",
  "permissions": [
    "sidePanel",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "side_panel": {
    "default_path": "index.html"
  },
  "content_scripts": [
  ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
